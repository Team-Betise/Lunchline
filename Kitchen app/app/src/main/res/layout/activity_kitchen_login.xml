<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="kitchenLoginVM" type="com.betise_lunchline_kitchen.app.modules.kitchenlogin.data.viewmodel.KitchenLoginVM"/>
    </data>
    <FrameLayout android:id="@+id/frameKitchenlogin"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <ImageView android:id="@+id/imageImageOne"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent"
            android:scaleType="fitXY" imageRes="@{@drawable/img_image1}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_image1"/>

        <LinearLayout
            android:id="@+id/linearColumnwelcometo"
            android:layout_width="match_parent"
            android:layout_height="361dp"
            android:layout_gravity="bottom|center"
            android:layout_marginStart="@dimen/_51pxh"
            android:layout_marginTop="@dimen/_257pxv"
            android:layout_marginEnd="@dimen/_51pxh"
            android:layout_marginBottom="@dimen/_257pxv"
            android:orientation="vertical">

            <TextView
                android:id="@+id/txtWelcometo"
                style="@style/txtInterregular18_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:layout_marginStart="@dimen/_46pxh"
                android:layout_marginEnd="@dimen/_46pxh"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@{kitchenLoginVM.kitchenLoginModel.txtWelcometo}"
                tools:text="@string/lbl_welcome_to" />

            <TextView
                android:id="@+id/txtLunchLineKitc"
                style="@style/txtHanumanbold36_1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_46pxh"
                android:layout_marginTop="@dimen/_36pxv"
                android:layout_marginEnd="@dimen/_46pxh"
                android:gravity="center_horizontal"
                android:text="@{kitchenLoginVM.kitchenLoginModel.txtLunchLineKitc}"
                tools:text="@string/msg_lunch_line_kitc" />

            <LinearLayout
                android:id="@+id/linearGooglelogin"
                style="@style/groupStylewhite_A700cornerRadius"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_90pxv"
                android:gravity="start"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/image2991148removeb"
                    android:layout_width="@dimen/_30pxh"
                    android:layout_height="@dimen/_30pxh"
                    android:layout_marginStart="@dimen/_29pxh"
                    android:layout_marginTop="@dimen/_6pxv"
                    android:layout_marginBottom="@dimen/_6pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_2991148removeb"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_2991148removeb" />

                <TextView
                    android:id="@+id/txtLoginwithEmai"
                    style="@style/txtInterregular18_2"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_24pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_65pxh"
                    android:layout_marginBottom="@dimen/_9pxv"
                    android:layout_weight="0.82"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:text="@{kitchenLoginVM.kitchenLoginModel.txtLoginwithEmai}"
                    tools:text="@string/msg_login_with_emai" />
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
