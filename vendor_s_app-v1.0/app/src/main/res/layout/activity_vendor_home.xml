<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="vendorHomeVM" type="com.betise_lunchline_vendor.app.modules.vendorhome.data.viewmodel.VendorHomeVM"/>
    </data>
    <LinearLayout android:gravity="center_horizontal"
        android:id="@+id/linearVendorhome"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylegray_901">
        <TextView android:ellipsize="end" android:id="@+id/txtWelcome"
            android:layout_gravity="center_horizontal"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_61pxh"
            android:layout_marginStart="@dimen/_61pxh"
            android:layout_marginTop="@dimen/_48pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{vendorHomeVM.vendorHomeModel.txtWelcome}"
            style="@style/txtInterregular18_1" tools:text="@string/lbl_welcome"/>
        <LinearLayout android:id="@+id/linearEdit"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_61pxh"
            android:layout_marginStart="@dimen/_61pxh"
            android:layout_marginTop="@dimen/_68pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegreen_901cornerRadius">
            <TextView android:ellipsize="end"
                android:id="@+id/txtHandoutItems"
                android:layout_gravity="start"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_7pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{vendorHomeVM.vendorHomeModel.txtHandoutItems}"
                style="@style/txtInterregular18_1" tools:text="@string/lbl_hand_out_items"/>
            <ImageView android:id="@+id/imageGroup48095580"
                android:layout_gravity="end"
                android:layout_height="@dimen/_76pxh"
                android:layout_marginBottom="@dimen/_3pxv"
                android:layout_marginEnd="@dimen/_12pxh"
                android:layout_marginStart="@dimen/_12pxh"
                android:layout_marginTop="@dimen/_38pxv"
                android:layout_width="@dimen/_130pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_group48095580"
                tools:ignore="ContentDescription" tools:src="@drawable/img_group48095580"/>
        </LinearLayout>
        <LinearLayout android:gravity="center_vertical"
            android:id="@+id/linearHandout"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_61pxh"
            android:layout_marginStart="@dimen/_61pxh"
            android:layout_marginTop="@dimen/_46pxv"
            android:layout_width="match_parent"
            android:orientation="horizontal" style="@style/groupStylegreen_901cornerRadius">
            <TextView android:ellipsize="end"
                android:id="@+id/txtEditMenu"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_83pxv"
                android:layout_marginStart="@dimen/_6pxh"
                android:layout_marginTop="@dimen/_42pxv"
                android:layout_weight="0.41" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@{vendorHomeVM.vendorHomeModel.txtEditMenu}"
                style="@style/txtInterregular18_1" tools:text="@string/lbl_edit_menu"/>
            <ImageView android:id="@+id/imageGroup48095581"
                android:layout_height="@dimen/_129pxh"
                android:layout_marginBottom="@dimen/_8pxv"
                android:layout_marginStart="@dimen/_57pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_width="@dimen/_119pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_group48095581"
                tools:ignore="ContentDescription" tools:src="@drawable/img_group48095581"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/linearHandout1"
            style="@style/groupStylegreen_901cornerRadius"
            android:layout_width="wrap_content"
            android:layout_height="164dp"
            android:layout_marginStart="@dimen/_61pxh"
            android:layout_marginTop="@dimen/_46pxv"
            android:layout_marginEnd="@dimen/_57pxh"
            android:layout_marginBottom="@dimen/_147pxv"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/txtAddItems"
                style="@style/txtInterregular18_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_6pxh"
                android:layout_marginTop="@dimen/_42pxv"
                android:layout_marginBottom="@dimen/_83pxv"
                android:layout_weight="0.44"
                android:ellipsize="end"
                android:singleLine="true"
                android:text="@{vendorHomeVM.vendorHomeModel.txtAddItems}"
                tools:text="@string/lbl_add_items" />

            <FrameLayout
                android:id="@+id/frameStackgroup48095581One"
                android:layout_width="@dimen/_109pxh"
                android:layout_height="@dimen/_129pxv"
                android:layout_marginStart="@dimen/_55pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginBottom="@dimen/_8pxv">

                <ImageView
                    android:id="@+id/imageGroup48095581One"
                    android:layout_width="@dimen/_86pxh"
                    android:layout_height="wrap_content"
                    android:layout_gravity="start"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_group48095581_green_a100"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_group48095581_green_a100" />

                <ImageView
                    android:id="@+id/imageMinimize"
                    android:layout_width="@dimen/_46pxh"
                    android:layout_height="@dimen/_43pxh"
                    android:layout_gravity="bottom|end"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_24pxv"
                    android:layout_marginBottom="@dimen/_24pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_minimize"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_minimize" />
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
