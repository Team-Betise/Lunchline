<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="otpVM" type="com.betise_lunchline_client.app.modules.otp.data.viewmodel.OtpVM"/>
    </data>
    <FrameLayout android:id="@+id/frameOTP"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylegray_900">
        <LinearLayout android:id="@+id/linearColumnarrowup"
            android:layout_gravity="bottom|start"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_11pxh"
            android:layout_marginStart="@dimen/_11pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="match_parent" android:orientation="vertical">
            <androidx.appcompat.widget.Toolbar android:gravity="start"
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearRowarrowup"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageArrowup"
                        android:layout_height="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="@dimen/_20pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_arrowup"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_arrowup"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtDetails"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_6pxv"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_weight="0.64"
                        android:layout_width="0dp"
                        android:lineSpacingExtra="@dimen/_1pxh"
                        android:singleLine="true"
                        android:text="@{otpVM.otpModel.txtDetails}"
                        style="@style/txtPoppinsmedium14" tools:text="@string/lbl_details"/>
                    <ImageView android:id="@+id/imageVector"
                        android:layout_height="@dimen/_4pxh"
                        android:layout_marginBottom="@dimen/_11pxv"
                        android:layout_marginStart="@dimen/_13pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="@dimen/_7pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_vector"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowvectortwentyseven"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_9pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="wrap_content" android:orientation="horizontal">

                <View
                    android:id="@+id/lineVectorTwentySeven"
                    android:layout_width="3dp"
                    android:layout_height="@dimen/_96pxv"
                    android:layout_weight="0.0"
                    android:background="@color/green_A700" />
                <LinearLayout
                    android:id="@+id/linearRowpngeggninetynine"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_22pxv"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_marginTop="@dimen/_2pxv"
                    android:layout_weight="1.0"
                    android:layout_width="0dp" android:orientation="horizontal">
                    <FrameLayout
                        android:id="@+id/frameStackpngeggninetynine"
                        android:layout_height="@dimen/_72pxv"
                        android:layout_width="@dimen/_66pxh" style="@style/groupStylewhite_1">
                        <ImageView
                            android:id="@+id/imagePngeggNinetyNine"
                            android:layout_gravity="top|start"
                            android:layout_height="@dimen/_65pxh"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginStart="@dimen/_5pxh"
                            android:layout_marginTop="@dimen/_1pxv"
                            android:layout_width="@dimen/_44pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_pngegg99"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_pngegg99"/>
                        <ImageView android:id="@+id/image77325frappucci"
                            android:layout_gravity="bottom|end"
                            android:layout_height="@dimen/_53pxh"
                            android:layout_marginBottom="@dimen/_4pxv"
                            android:layout_marginEnd="@dimen/_4pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="@dimen/_43pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_77325frappucci"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_77325frappucci"/>
                        <ImageView
                            android:id="@+id/imagePngeggThirtyTwo"
                            android:layout_gravity="bottom|end"
                            android:layout_height="@dimen/_25pxh"
                            android:layout_marginBottom="@dimen/_2pxv"
                            android:layout_marginEnd="@dimen/_7pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="@dimen/_24pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_pngegg32"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_pngegg32"/>
                    </FrameLayout>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearColumnfooditem"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_32pxv"
                        android:layout_marginStart="@dimen/_16pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_weight="0.48"
                        android:layout_width="0dp" android:orientation="vertical">

                        <TextView
                            android:id="@+id/txtFoodItem"
                            style="@style/txtPoppinsmedium12"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:ellipsize="end"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{otpVM.otpModel.txtFoodItem}"
                            tools:text="@string/lbl_food_item" />
                        <TextView android:ellipsize="end"
                            android:id="@+id/txtPrice"
                            android:layout_height="wrap_content"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_5pxv"
                            android:layout_width="wrap_content"
                            android:lineSpacingExtra="@dimen/_1pxh"
                            android:singleLine="true"
                            android:text="@{otpVM.otpModel.txtPrice}"
                            style="@style/txtPoppinsmedium14" tools:text="@string/lbl_35"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <TextView
                android:id="@+id/txtOTP"
                style="@style/txtIntersemibold16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="end"
                android:layout_marginStart="@dimen/_50pxh"
                android:layout_marginTop="@dimen/_129pxv"
                android:layout_marginEnd="@dimen/_110pxh"
                android:ellipsize="end"
                android:lineSpacingExtra="@dimen/_1pxh"
                android:singleLine="true"
                android:text="@{otpVM.otpModel.txtOTP}"
                tools:text="@string/msg_otp_for_your_or" />
            <LinearLayout android:gravity="end"
                android:id="@+id/linearRowframefour"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_15pxh"
                android:layout_marginStart="@dimen/_15pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFrameFour"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.25"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:paddingBottom="@dimen/_18pxv"
                    android:paddingEnd="@dimen/_20pxh"
                    android:paddingStart="@dimen/_19pxh"
                    android:paddingTop="@dimen/_20pxv"
                    android:singleLine="true"
                    android:text="@{otpVM.otpModel.txtFrameFour}"
                    style="@style/txtSolidRounded_1" tools:text="@string/lbl_x"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFrameFive"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_weight="0.25"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:paddingBottom="@dimen/_18pxv"
                    android:paddingEnd="@dimen/_20pxh"
                    android:paddingStart="@dimen/_19pxh"
                    android:paddingTop="@dimen/_20pxv"
                    android:singleLine="true"
                    android:text="@{otpVM.otpModel.txtFrameFive}"
                    style="@style/txtSolidRounded_1" tools:text="@string/lbl_x"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFrameSix"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_weight="0.25"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:paddingBottom="@dimen/_18pxv"
                    android:paddingEnd="@dimen/_20pxh"
                    android:paddingStart="@dimen/_19pxh"
                    android:paddingTop="@dimen/_20pxv"
                    android:singleLine="true"
                    android:text="@{otpVM.otpModel.txtFrameSix}"
                    style="@style/txtSolidRounded_1" tools:text="@string/lbl_x"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFrameSeven"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_weight="0.25"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:paddingBottom="@dimen/_18pxv"
                    android:paddingEnd="@dimen/_20pxh"
                    android:paddingStart="@dimen/_19pxh"
                    android:paddingTop="@dimen/_20pxv"
                    android:singleLine="true"
                    android:text="@{otpVM.otpModel.txtFrameSeven}"
                    style="@style/txtSolidRounded_1" tools:text="@string/lbl_x"/>
            </LinearLayout>

            <TextView
                android:id="@+id/txtEnjoyyourmeal"
                style="@style/txtPoppinsmedium18"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_110pxh"
                android:layout_marginTop="@dimen/_101pxv"
                android:layout_marginEnd="@dimen/_47pxh"
                android:ellipsize="end"
                android:gravity="end"
                android:singleLine="true"
                android:text="@{otpVM.otpModel.txtEnjoyyourmeal}"
                tools:text="@string/msg_enjoy_your_meal" />

            <androidx.appcompat.widget.AppCompatButton
                android:id="@+id/btnOkay"
                style="@style/btnSolidRounded_1"
                android:layout_width="@dimen/_234pxh"
                android:layout_height="@dimen/_57pxv"
                android:layout_gravity="end"
                android:layout_marginStart="@dimen/_1pxh"
                android:layout_marginTop="@dimen/_100pxv"
                android:ellipsize="end"
                android:gravity="center"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_18pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingBottom="@dimen/_8pxv"
                android:singleLine="true"
                android:text="@string/lbl_okay"
                android:textAllCaps="false"
                android:translationX="-50dp"
                tools:text="@string/lbl_okay" />
        </LinearLayout>
    </FrameLayout>
</layout>
