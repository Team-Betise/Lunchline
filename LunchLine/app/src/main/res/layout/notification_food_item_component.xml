<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginTop="@dimen/_50pxv"
    android:layout_marginBottom="@dimen/_50pxv">
    <LinearLayout
        android:id="@+id/linearRowarrowright"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_2pxh"
        android:orientation="horizontal">

        <LinearLayout
            android:id="@+id/linearRowpngeggninetynine"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.94"
            android:orientation="horizontal">

            <FrameLayout
                android:id="@+id/foodImageContainer"
                style="@style/groupStylewhite_1"
                android:layout_width="@dimen/_66pxh"
                android:layout_height="@dimen/_72pxv">

                <ImageView
                    android:id="@+id/imagePngeggNinetyNine"
                    android:layout_width="@dimen/_44pxh"
                    android:layout_height="@dimen/_65pxh"
                    android:layout_gravity="top|start"
                    android:layout_marginStart="@dimen/_5pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_pngegg99"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_pngegg99" />

                <ImageView
                    android:id="@+id/image77325frappucci"
                    android:layout_width="@dimen/_43pxh"
                    android:layout_height="@dimen/_53pxh"
                    android:layout_gravity="bottom|end"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_4pxh"
                    android:layout_marginBottom="@dimen/_4pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_77325frappucci"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_77325frappucci" />

                <ImageView
                    android:id="@+id/imagePngeggThirtyTwo"
                    android:layout_width="@dimen/_24pxh"
                    android:layout_height="@dimen/_25pxh"
                    android:layout_gravity="bottom|end"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_marginEnd="@dimen/_7pxh"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_pngegg32"
                    tools:ignore="ContentDescription"
                    tools:src="@drawable/img_pngegg32" />
            </FrameLayout>

            <LinearLayout
                android:id="@+id/foodDataContainer"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_5pxv"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_weight="0.51"
                android:gravity="start"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/foodNameField"
                    style="@style/txtPoppinsmedium12"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_7pxh"
                    android:ellipsize="end"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{notificationCompleteVM.notificationCompleteModel.txtFoodItem}"
                    tools:text="@string/lbl_food_item" />

                <TextView
                    android:id="@+id/foodPriceField"
                    style="@style/txtPoppinsmedium14"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_5pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:ellipsize="end"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@{notificationCompleteVM.notificationCompleteModel.txtPrice}"
                    tools:text="@string/lbl_35" />

                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/foodQuantityField"
                    style="@style/btnSolidRounded_2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:paddingStart="@dimen/_30pxh"
                    android:paddingTop="@dimen/_6pxv"
                    android:paddingEnd="@dimen/_30pxh"
                    android:paddingBottom="@dimen/_6pxv"
                    android:singleLine="true"
                    android:text="@string/lbl_2"
                    android:textAllCaps="false"
                    tools:text="@string/lbl_2" />
            </LinearLayout>
        </LinearLayout>

        <ImageView
            android:id="@+id/imageArrowright"
            android:layout_width="@dimen/_10pxh"
            android:layout_height="@dimen/_18pxh"
            android:layout_marginStart="@dimen/_151pxh"
            android:layout_marginTop="@dimen/_23pxv"
            android:layout_marginBottom="@dimen/_31pxv"
            android:scaleType="fitXY"
            android:src="@drawable/img_arrowright"
            tools:ignore="ContentDescription"
            tools:src="@drawable/img_arrowright" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/foodStatusFieldContainer1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_2pxh"
        android:layout_marginTop="@dimen/_1pxv"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/foodStatusField1"
            style="@style/txtPoppinsmedium15_1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.89"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="Order received"
            tools:text="@string/lbl_order_recieved" />

        <ImageView
            android:id="@+id/imageCheckmark"
            android:layout_width="@dimen/_13pxh"
            android:layout_height="@dimen/_13pxh"
            android:layout_marginStart="@dimen/_187pxh"
            android:layout_marginBottom="@dimen/_4pxv"
            android:scaleType="fitXY"
            android:src="@drawable/img_checkmark"
            tools:ignore="ContentDescription"
            tools:src="@drawable/img_checkmark" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/foodStatusFieldContainer2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_2pxh"
        android:layout_marginTop="@dimen/_6pxv"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/foodStatusField2"
            style="@style/txtPoppinsmedium15_1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.94"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="@{notificationCompleteVM.notificationCompleteModel.txtLanguageOne}"
            tools:text="@string/msg_food_item_is" />

        <ImageView
            android:id="@+id/imageCheckmarkOne"
            android:layout_width="@dimen/_13pxh"
            android:layout_height="@dimen/_13pxh"
            android:layout_marginStart="@dimen/_84pxh"
            android:layout_marginBottom="@dimen/_5pxv"
            android:scaleType="fitXY"
            android:src="@drawable/img_checkmark"
            tools:ignore="ContentDescription"
            tools:src="@drawable/img_checkmark" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/foodStatusFieldContainer3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_2pxh"
        android:layout_marginTop="@dimen/_4pxv"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/foodStatusField3"
            style="@style/txtPoppinsmedium15_1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.94"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="@{notificationCompleteVM.notificationCompleteModel.txtFoodItembei}"
            tools:text="@string/msg_food_item_bei" />

        <ImageView
            android:id="@+id/imageCheckmarkTwo"
            android:layout_width="@dimen/_13pxh"
            android:layout_height="@dimen/_13pxh"
            android:layout_marginStart="@dimen/_110pxh"
            android:layout_marginBottom="@dimen/_5pxv"
            android:scaleType="fitXY"
            android:src="@drawable/img_checkmark"
            tools:ignore="ContentDescription"
            tools:src="@drawable/img_checkmark" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/foodStatusFieldContainer4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_2pxh"
        android:layout_marginTop="@dimen/_4pxv"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/foodStatusField4"
            style="@style/txtPoppinsmedium15_1"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="0.92"
            android:ellipsize="end"
            android:singleLine="true"
            android:text="@{notificationCompleteVM.notificationCompleteModel.txtFoodItemis}"
            tools:text="@string/msg_food_item_is2" />

        <ImageView
            android:id="@+id/imageCheckmarkThree"
            android:layout_width="@dimen/_13pxh"
            android:layout_height="@dimen/_13pxh"
            android:layout_marginStart="@dimen/_142pxh"
            android:layout_marginBottom="@dimen/_4pxv"
            android:scaleType="fitXY"
            android:src="@drawable/img_checkmark"
            tools:ignore="ContentDescription"
            tools:src="@drawable/img_checkmark" />
    </LinearLayout>
</LinearLayout>